{
    "title": "SELECT dept, id WHERE dept begin with a && pass == 100 ORDER BY id",
    "query": {
    	"WHERE": {
    		"AND": [
    			{
    				"IS": {
    					"courses_dept": "a*"
    				}
    			},
    			{
    				"GT": {
    					"courses_pass": 800
    				}
    			}
    		]
    	},
    	"OPTIONS": {
    		"COLUMNS": [
    			"courses_dept",
    			"courses_id"
    		],
    		"ORDER": "courses_id"
    	}
    },
    "isQueryValid": true,
